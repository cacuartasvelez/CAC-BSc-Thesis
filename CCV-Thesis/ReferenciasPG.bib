  volume = {41},
  url = {http://ao.osa.org/abstract.cfm?URI=ao-41-11-2095},
  doi = {10.1364/AO.41.002095},
  abstract = {We show in benchtop experiments that wave-front phase estimation by phase diversity can be significantly improved by simultaneous amplitude estimation. Processing speed, which will be important for real-time wave-front control applications, can be enhanced by use of small-format detectors with pixels that do not fully sample the diffraction limit. Using an object-independent phase-diversity algorithm, we show that, for both pointlike and extended objects, the fidelity of the phase and amplitude estimates degrades gracefully, rather than catastrophically, as the sampling becomes coarser. We show in simulation that the same algorithm also improves the fidelity of image reconstruction of complex targets.},
  timestamp = {2015-04-23 15:23:28},
  number = {11},
  urldate = {2015-04-23},
  journal = {Appl. Opt.},
  author = {Jefferies, Stuart M. and Lloyd-Hart, Michael and Hege, E. Keith and Georges, James},
  year = {2002},
  keywords = {Active or adaptive optics,Inverse problems,Phase measurement,Phase retrieval},
  pages = {2095--2102},
  file = {Appl. Opt. Snapshot:/home/ccuartas/.zotero/zotero/wqheqv22.default/zotero/storage/87VKDJ7T/abstract.html:text/html;Jefferies et al_2002_Sensing wave-front amplitude and phase with phase diversity.pdf:/home/ccuartas/Documentos/Art. Zotero//Applied Optics/2002/Jefferies et al_2002_Sensing wave-front amplitude and phase with phase diversity.pdf:application/pdf}
