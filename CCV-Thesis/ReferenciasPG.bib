  keywords = {Heme,Hemeproteins,Hydrogen-Ion Concentration,Iron,Leeches,Macromolecular Substances,Mercaptoethanol,Molecular Weight,Oxygen,Protein Conformation,Sodium Dodecyl Sulfate},
  pages = {202--213}
}

